{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Run Extension",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "skipFiles": ["<node_internals>/**"],
            "preLaunchTask": "${defaultBuildTask}",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"]
        },
    ]
}